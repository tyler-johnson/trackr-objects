{
	"name": "trackr-objects",
	"version": "0.2.0",
	"description": "Turns JavaScript objects and arrays into reactive resources for Trackr.",
	"author": "Tyler Johnson <tyler@tylerjohnson.me>",
	"main": "index.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/tyler-johnson/trackr-objects.git"
	},
	"scripts": {
		"lint": "jshint index.js",
		"build-js": "browserify ./src/index.js -t babelify --standalone track > dist/trackr-objects.js",
		"build-min-js": "browserify ./src/index.js -t babelify --standalone track | uglifyjs > dist/trackr-objects.min.js",
		"build": "rm -rf dist/ && mkdir -p dist/ && npm run build-js && npm run build-min-js"
	},
	"dependencies": {
		"trackr": "~2.0.0",
		"array-spy": "~0.1.0",
		"has-own-prop": "~1.0.0",
		"is-plain-object": "~2.0.1",
		"shallow-copy": "0.0.1"
	},
	"devDependencies": {
		"babel": "^5.8.23",
		"babelify": "^6.3.0",
		"browserify": "~11.0.1",
		"jshint": "~2.8.0",
		"uglify-js": "~2.4.24"
	},
	"license": "MIT",
	"keywords": [
		"trackr",
		"objects",
		"array",
		"reactive"
	]
}
